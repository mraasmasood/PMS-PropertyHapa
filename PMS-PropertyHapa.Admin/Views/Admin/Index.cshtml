                                    <div class="col-xxl-3 col-md-6">
                                        <div>
                                            <label for="OrganizationInfo_OrganizatioPrimaryColor">Primary Color:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                            <input type="color" id="OrganizationInfo_OrganizatioPrimaryColor" name="OrganizationInfo.OrganizatioPrimaryColor" class="form-control" /> 
                                            <span asp-validation-for="OrganizationInfo.OrganizatioPrimaryColor" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <!-- Organization Secondary Color -->
                                    <div class="col-xxl-3 col-md-6">
                                        <div>
                                            <label for="OrganizationInfo_OrganizationSecondColor">Secondary Color:&nbsp;<span style="color:red!important"><b>*</b></span></label>
