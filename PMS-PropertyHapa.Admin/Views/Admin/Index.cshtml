    }


    function createTenant() {
    
        

        var formData = new FormData();
        formData.append('User.FirstName', $('#User_FirstName').val());
        formData.append('User.LastName', $('#User_LastName').val());
        formData.append('User.Email', $('#User_Email').val());
        formData.append('User.PhoneNumber', $('#User_PhoneNumber').val());
        formData.append('User.BirthDate', $('#User_BirthDate').val());
        formData.append('User.UserName', $('#User_UserName').val());
        formData.append('User.Password', $('#User_Password').val());
        formData.append('User.Group', $('#User_Group').val());
        formData.append('OrganizationInfo.OrganizationName', $('#OrganizationInfo_OrganizationName').val());
        formData.append('OrganizationInfo.OrganizationDescription', $('#OrganizationInfo_OrganizationDescription').val());
        formData.append('OrganizationInfo.OrganizationIconFile', $('#OrganizationInfo_OrganizationIconFile')[0].files[0]);
        formData.append('OrganizationInfo.OrganizationLogoFile', $('#OrganizationInfo_OrganizationLogoFile')[0].files[0]);
        formData.append('OrganizationInfo.OrganizatioPrimaryColor', $('#OrganizationInfo_OrganizatioPrimaryColor').val());
        formData.append('OrganizationInfo.OrganizationSecondColor', $('#OrganizationInfo_OrganizationSecondColor').val());

        $.ajax({
            type: 'POST',
            url: '/Admin/Create', 
            data: formData,
            contentType: false, 
            processData: false,
            success: function (response) {
                if (response) {
                    var Toast = Swal.mixin({ toast: true, position: 'top-end', showConfirmButton: false, progressBar: true, timer: 3000 });
                    Toast.fire({ icon: 'success', title: 'Tenant created Successfully..!' });
                    Refresh();
                } else {
                    var Toast = Swal.mixin({ toast: true, position: 'top-end', showConfirmButton: false, progressBar: true, timer: 3000 });
                    Toast.fire({ icon: 'error', title: 'Failed to create Tenant' });
                }
            },
            error: function (xhr, status, error) {
                var Toast = Swal.mixin({ toast: true, position: 'top-end', showConfirmButton: false, progressBar: true, timer: 3000 });
                Toast.fire({ icon: 'error', title: 'An error Occurred' });
            }
        });
    }

    
    $('#createTenantButton').on('click', function (e) {
        e.preventDefault(); 
        createTenant();
    });
</script>

