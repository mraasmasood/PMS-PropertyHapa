                        <h4 class="card-title mb-0 flex-grow-1">Create New Organization</h4>
                    </div>
                    <div class="card-body">
                        <form class="create-form" novalidate="" method="post" enctype="multipart/form-data">
                        <div class="live-preview">
                            <div class="row gy-4">
                                <!--end col-->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>First Name:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <input asp-for="User.FirstName" class="form-control" placeholder="Enter First Name"  />
                                        <span asp-validation-for="User.FirstName" class="text-danger"></span>
                                    </div>
                                </div>
                                <!--end col-->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label asp-for="User.LastName"></label>
                                        <input asp-for="User.LastName" class="form-control" placeholder="Enter Last Name"  />
                                        <span asp-validation-for="User.LastName" class="text-danger"></span>
                                    </div>
                                </div>
                                <!--end col-->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>User Name:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <input asp-for="User.UserName" class="form-control" placeholder="Please Enter Username" autocomplete="off" />
                                        <span asp-validation-for="User.UserName" class="text-danger"></span>
                                    </div>
                                </div>
                                <!--end col-->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>Email:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <div class="form-icon right">
                                            <input type="email" class="form-control form-control-icon" asp-for="User.Email" placeholder="example@gmail.com">
                                            <i class="ri-mail-unread-line"></i>
                                        </div>
                                        <span asp-validation-for="User.Email" class="text-danger"></span>
                                    </div>
                                </div>
                                <!--end col-->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>Password:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <div class="form-icon right">
                                            <input asp-for="User.Password" class="form-control form-control-icon" placeholder="Ex: Test@123" />
                                            <i class="ri-lock-password-line"></i>
                                        </div>
                                        <span asp-validation-for="User.Password" class="text-danger"></span>
                                    </div>
                                </div>
                                <!--end col-->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label asp-for="User.BirthDate"></label>
                                        <div class="form-icon right">
                                                <input asp-for="User.BirthDate" name="User.BirthDate" type="date" class="form-control" />

                                        </div>
                                        <span asp-validation-for="User.BirthDate" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>PhoneNumber:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <div class="form-icon right">
                                            <input asp-for="User.PhoneNumber" class="form-control form-control-icon" placeholder="Please Enter Phone Number" type="number" />
                                            <i class="ri-phone-line"></i>
                                        </div>
                                        <span asp-validation-for="User.PhoneNumber" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>Role</label>
                                            <select asp-for="User.Group" id="User_Group" class="form-control"></select>
                                    </div>
                                </div>
                                <!-- Organization Name -->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>Organization Name:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <input asp-for="OrganizationInfo.OrganizationName" class="form-control" placeholder="Enter Organization Name" />
                                        <span asp-validation-for="OrganizationInfo.OrganizationName" class="text-danger"></span>
                                    </div>
                                </div>

                                <!-- Organization Description -->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>Organization Description:</label>
                                        <textarea asp-for="OrganizationInfo.OrganizationDescription" class="form-control" placeholder="Enter Organization Description"></textarea>
                                        <span asp-validation-for="OrganizationInfo.OrganizationDescription" class="text-danger"></span>
                                    </div>
                                </div>

                                <!-- Organization Icon File Upload -->
                                <div class="col-xxl-3 col-md-6">
                                    <div>
                                        <label>Organization Icon:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <input asp-for="OrganizationInfo.OrganizationIconFile" type="file" class="form-control" />
                                        <span asp-validation-for="OrganizationInfo.OrganizationIconFile" class="text-danger"></span>
                                    </div>
                                </div>

                                <!-- Organization Logo File Upload -->
