                                    <div>
                                        <label>Organization Logo:&nbsp;<span style="color:red!important"><b>*</b></span></label>
                                        <input asp-for="OrganizationInfo.OrganizationLogoFile" type="file" class="form-control" />
                                            <span asp-validation-for="OrganizationInfo.OrganizationLogoFile" class="text-danger"></span>
                                    </div>
                                </div>

