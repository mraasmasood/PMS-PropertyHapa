<script>
    var tenantId = localStorage.getItem('userId');
    $(document).ready(function () {
   
        var ajaxUrl = `/Tenant/GetTenant/?tenantId=${encodeURIComponent(tenantId)}`;
        fetchTenants();

        function fetchTenants() {
            console.log('Fetching tenants...');
            $.ajax({
                url: ajaxUrl,
                type: 'GET',
                dataType: 'json',
                success: function (data) {
                    console.log('Data received:', data);
                    populateTenants(data);
                },
                error: function (error) {
                    console.error("Error fetching tenants:", error);
                }
            });
        }
        function populateTenants(tenants) {
            var tenantsHtml = '';
            var detailsHtml = '';
            var defaultImageBase64 = 'data:image/jpeg;base64,';
            tenants.forEach(function (tenant, index) {
                var isActive = index === 0 ? 'active' : ''; // Make the first tab active
                var tenantPicture = tenant.Picture || defaultImageBase64;

                tenantsHtml += '<a class="nav-link ' + isActive + '" id="v-pills-' + tenant.TenantId + '-tab" data-bs-toggle="pill" href="#v-pills-' + tenant.TenantId + '" role="tab" aria-controls="v-pills-' + tenant.TenantId + '" aria-selected="true">' +
                    '<div class="media">' +
                    '<img class="img-50 img-fluid m-r-20 rounded-circle" src="' + tenantPicture + '" alt="">' +
                    '<div class="media-body">' +
                    '<h6>' + tenant.FirstName + ' ' + tenant.LastName + '</h6>' +
                    '<p>' + tenant.EmailAddress + '</p>' +
                    '</div>' +
                    '</div>' +
                    '</a>';

                detailsHtml += '<div class="tab-pane fade ' + isActive + ' show" id="v-pills-' + tenant.TenantId + '" role="tabpanel" aria-labelledby="v-pills-' + tenant.TenantId + '-tab">' +
                    '<div class="profile-mail">' +
                    '<div class="media align-items-center">' +
                    '<img class="img-100 img-fluid m-r-20 rounded-circle" src="' + tenantPicture + '" alt="">' +
                    '<div class="media-body mt-0">' +
                    '<h5>' + tenant.FirstName + ' ' + tenant.LastName + '</h5>' +
                    '<p>' + tenant.EmailAddress + '</p>' +
                    '<div class="mt-2">' +
                    '<a href="javascript:void(0);" onclick="editTenant(' + tenant.TenantId + ');" style="margin-right: 10px;">Edit</a>' +
                    '<a href="javascript:void(0);" onclick="deleteTenant(' + tenant.TenantId + ');">Delete</a>' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '<div class="email-general">' +
                    '<h6 class="mb-3">General</h6>' +
                    '<ul>' +
                    '<li>First Name: <span class="font-primary">' + tenant.FirstName + '</span></li>' +
                    '<li>Last Name: <span class="font-primary">' + tenant.LastName + '</span></li>' +
                    '<li>Email Address: <span class="font-primary">' + tenant.EmailAddress + '</span></li>' +
                    '<li>Phone Number: <span class="font-primary">' + tenant.PhoneNumber + '</span></li>' +
                    '<li>Emergency Contact Info: <span class="font-primary">' + tenant.EmergencyContactInfo + '</span></li>' +
                    '<li>Lease Agreement Id: <span class="font-primary">' + tenant.LeaseAgreementId + '</span></li>' +
                    '<li>Tenant Nationality: <span class="font-primary">' + tenant.TenantNationality + '</span></li>' +
                    '<li>Gender: <span class="font-primary">' + tenant.Gender + '</span></li>' +
                    '<li>DOB: <span class="font-primary">' + tenant.DOB + '</span></li>' +
                    '<li>VAT: <span class="font-primary">' + tenant.VAT + '</span></li>' +
                    '<li>Legal Name: <span class="font-primary">' + tenant.LegalName + '</span></li>' +
                    '<li>Account Name: <span class="font-primary">' + tenant.Account_Name + '</span></li>' +
                    '<li>Account Holder: <span class="font-primary">' + tenant.Account_Holder + '</span></li>' +
                    '<li>Account IBAN: <span class="font-primary">' + tenant.Account_IBAN + '</span></li>' +
                    '<li>Account Swift: <span class="font-primary">' + tenant.Account_Swift + '</span></li>' +
                    '<li>Account Bank: <span class="font-primary">' + tenant.Account_Bank + '</span></li>' +
                    '<li>Account Currency: <span class="font-primary">' + tenant.Account_Currency + '</span></li>' +
                    '<li>Address: <span class="font-primary">' + tenant.Address + '</span></li>' +
                    '<li>Address2: <span class="font-primary">' + tenant.Address2 + '</span></li>' +
                    '<li>Locality: <span class="font-primary">' + tenant.Locality + '</span></li>' +
                    '<li>Region: <span class="font-primary">' + tenant.Region + '</span></li>' +
                    '<li>Postal Code: <span class="font-primary">' + tenant.PostalCode + '</span></li>' +
                    '<li>Country: <span class="font-primary">' + tenant.Country + '</span></li>' +
                    '<li>Country Code: <span class="font-primary">' + tenant.CountryCode + '</span></li>' +
                    '</ul>' +
                    '</div>' +
                    '</div>' +
                    '</div>';
            });

            $('#v-pills-tab').html(tenantsHtml);
            $('#v-pills-tabContent').html(detailsHtml);

            $('#tenantListCount').text(tenants.length + ' List');
        }

    });
</script>

<script>



    function deleteTenant(tenantId) {
        Swal.fire({
            title: 'Are you sure?',
            text: 'Once deleted, you will not be able to recover this tenant!',
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, delete it!'
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    url: `/Tenant/Delete/?tenantId=${encodeURIComponent(tenantId)}`,
                    type: 'DELETE',
                    success: function (data) {
                        if (data.success) {
                            window.location.reload();
                            Swal.fire(
                                'Deleted!',
                                'The tenant has been deleted.',
                                'success'
                            );
                        } else {
                            Swal.fire(
                                'Error!',
                                'There was an issue deleting the tenant.',
                                'error'
                            );
                        }
                    },
                    error: function () {
                        Swal.fire(
                            'Error!',
                            'There was an issue deleting the tenant.',
                            'error'
                        );
                    }
                });
            }
        });
    }





</script>
<script>
    function editTenant(tenantId) {
